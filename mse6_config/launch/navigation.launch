<?xml version="1.0" ?>
<launch>
    
    <arg name="robot" default="mse6_00000"/>    
    
    <group ns="navigation">
        
        <node pkg="move_base" type="move_base" name="move_base" output="screen">
        
            <rosparam file="$(find mse6_config)/config/navigation/move_base/costmap_common_params.yaml" command="load" ns="global_costmap" subst_value="true"/> 
            <rosparam file="$(find mse6_config)/config/navigation/move_base/costmap_common_params.yaml" command="load" ns="local_costmap" subst_value="true"/>
            <rosparam file="$(find mse6_config)/config/navigation/move_base/local_costmap_params.yaml" command="load" subst_value="true"/>
            <rosparam file="$(find mse6_config)/config/navigation/move_base/global_costmap_params.yaml" command="load" subst_value="true"/> 
            <rosparam file="$(find mse6_config)/config/navigation/move_base/move_base_params.yaml" command="load" subst_value="true"/>
            
            <param name="base_local_planner" type="str" value="teb_local_planner/TebLocalPlannerROS"/>
            <!--param name="base_local_planner" type="str" value="dwa_local_planner/DWAPlannerROS"/-->
            
            <remap from="cmd_vel" to="/$(arg robot)/mobile_base_controller/cmd_vel"/>                    
            <remap from="map" to="/$(arg robot)/localization/map"/>                                    
        </node>
        
        
    </group>
            
</launch>
